    faction = GA_FactionCardDictionary(
      this.generateScoiataelRulesHeroCards(),
      this.generateScoiataelRulesLeaderCards(),
      this.generateScoiataelRulesUnitCards()
    );

    return faction;
  }

  function generateScoiataelRulesHeroCards(): array<GA_CardEntry> {
    var output: array<GA_CardEntry>;
    return output;
  }
  function generateScoiataelRulesLeaderCards(): array<GA_CardEntry> {
    var output: array<GA_CardEntry>;
    return output;
  }
  function generateScoiataelRulesUnitCards(): array<GA_CardEntry> {
    var output: array<GA_CardEntry>;
    return output;
  }

  //////////////////////////////////////////////////////////////////////////////

  function generateMonsterRules(): GA_FactionCardDictionary {
    var faction: GA_FactionCardDictionary;

    faction = GA_FactionCardDictionary(
      this.generateMonsterRulesHeroCards(),
      this.generateMonsterRulesLeaderCards(),
      this.generateMonsterRulesUnitCards()
    );

    return faction;
  }

