    faction = GA_FactionCardDictionary(
      this.generateNilfgaardianRulesHeroCards(),
      this.generateNilfgaardianRulesLeaderCards(),
      this.generateNilfgaardianRulesUnitCards()
    );

    return faction;
  }

  function generateNilfgaardianRulesHeroCards(): array<GA_CardEntry> {
    var output: array<GA_CardEntry>;
    return output;
  }
  function generateNilfgaardianRulesLeaderCards(): array<GA_CardEntry> {
    var output: array<GA_CardEntry>;
    return output;
  }
  function generateNilfgaardianRulesUnitCards(): array<GA_CardEntry> {
    var output: array<GA_CardEntry>;
    return output;
  }

  //////////////////////////////////////////////////////////////////////////////

  function generateScoiataelRules(): GA_FactionCardDictionary {
