    faction = GA_FactionCardDictionary(
      this.generateSkelligeRulesHeroCards(),
      this.generateSkelligeRulesLeaderCards(),
      this.generateSkelligeRulesUnitCards()
    );

    return faction;
  }

  function generateSkelligeRulesHeroCards(): array<GA_CardEntry> {
