
abstract class GA_BaseRulesetGenerator {
  //////////////////////////////////////////////////////////////////////////////
  // The following functions are meant to be overridden by the generated code //
  // or any class that extends this class to return custom generation rules.  //
  //                                                                          //
  // Each faction has a main function that calls three sub-functions to get   //
  // the hero, leader, unit cards. When overriding the class it is enough to  //
  // override the sub-functions only.                                         //
  //////////////////////////////////////////////////////////////////////////////

  function generateNorthernKingdomRules(): GA_FactionCardDictionary {
    var faction: GA_FactionCardDictionary;

    faction = GA_FactionCardDictionary(
      this.generateNorthernKingdomRulesHeroCards(),
      this.generateNorthernKingdomRulesLeaderCards(),
      this.generateNorthernKingdomRulesUnitCards()
    );

    return faction;
  }

  function generateNorthernKingdomRulesHeroCards(): array<GA_CardEntry> {
    var output: array<GA_CardEntry>;
    return output;
  }
  function generateNorthernKingdomRulesLeaderCards(): array<GA_CardEntry> {
    var output: array<GA_CardEntry>;
    return output;
  }
  function generateNorthernKingdomRulesUnitCards(): array<GA_CardEntry> {
    var output: array<GA_CardEntry>;
    return output;
  }

  //////////////////////////////////////////////////////////////////////////////

  function generateSkelligeRules(): GA_FactionCardDictionary {
